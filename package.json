        "vite-plugin-wasm": "3.2.2",
        "zx": "7.2.2"
    },
    "//": "dedupe packages for pnpm",
    "pnpm": {
        "overrides": {
            "highlight.js": "11.8.0",
            "@codemirror/lang-markdown": "6.1.1",
            "@codemirror/language-data": "6.3.1",
            "@codemirror/state": "6.2.1",
            "@codemirror/view": "6.13.2",
            "react": "18.2.0",
            "react-dom": "18.2.0",
            "@mantine/core": "6.0.14",
            "@mantine/hooks": "6.0.14",
            "ts-pattern": "5.0.1",
            "typescript": "5.1.3"
        }
    }
}
