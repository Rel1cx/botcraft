{
    "name": "botcraft",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "pnpm run i18n && vite build",
        "check": "tsx ./scripts/check.ts",
        "clean": "rm -rf docs dist node_modules pnpm-lock.yaml",
        "dev": "vite",
        "format": "rome format . --write && pnpm check",
        "i18n": "typesafe-i18n --no-watch",
        "preinstall": "npx only-allow pnpm",
        "lint": "rome format . --write && pnpm eslint --color src --fix",
        "list-unused": "ts-unused-exports ./tsconfig.json --excludePathsFromReport=src/i18n --exitWithCount",
        "prepare": "husky install",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit --extendedDiagnostics -p .",
        "docs": "typedoc --options ./typedoc.json"
    },
    "browserslist": [
        "defaults and supports es6-module"
    ],
    "dependencies": {
        "@ariakit/react": "0.2.12",
        "@codemirror/lang-markdown": "6.2.0",
        "@codemirror/language-data": "6.3.1",
        "@codemirror/state": "6.2.1",
        "@codemirror/view": "6.14.0",
        "@dqbd/tiktoken": "1.0.7",
        "@floating-ui/react": "0.24.5",
        "@mantine/core": "6.0.16",
        "@mantine/hooks": "6.0.16",
        "@phosphor-icons/react": "2.0.10",
        "@radix-ui/react-slot": "1.0.2",
        "@react-hookz/web": "23.1.0",
        "@rizzzse/bimap": "1.2.1",
        "@scure/bip39": "1.2.1",
        "@swan-io/boxed": "1.0.2",
        "@swan-io/chicane": "1.4.1",
        "@total-typescript/ts-reset": "0.4.2",
        "@uiw/react-codemirror": "4.21.7",
        "bind-event-listener": "3.0.0",
        "clsx": "1.2.1",
        "cm6-theme-basic-light": "0.2.0",
        "codemirror": "6.0.1",
        "core-js": "3.31.0",
        "date-fns": "2.30.0",
        "deep-weak-map": "1.1.0",
        "dnum": "2.7.0",
        "fast-equals": "5.0.1",
        "framer-motion": "10.12.18",
        "highlight.js": "11.8.0",
        "idb-keyval": "6.2.1",
        "immer": "10.0.2",
        "jotai": "2.2.2",
        "jotai-game": "0.1.0",
        "jotai-immer": "0.2.0",
        "jotai-minidb": "0.0.6",
        "katex": "0.16.8",
        "ky": "0.33.3",
        "nominal-types": "0.2.0",
        "rambda": "8.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-error-boundary": "4.0.10",
        "react-hot-toast": "2.4.1",
        "react-hotkeys-hook": "4.4.1",
        "react-intersection-observer": "9.5.2",
        "react-markdown": "8.0.7",
        "rehype-highlight": "6.0.0",
        "rehype-katex": "6.0.3",
        "remark-breaks": "3.0.3",
        "remark-gfm": "3.0.1",
        "remark-math": "5.1.1",
        "rsup-progress": "3.1.1",
        "rxjs": "7.8.1",
        "short-unique-id": "4.4.4",
        "telejson": "7.1.0",
        "throttle-debounce": "5.0.0",
        "tilg": "0.1.1",
        "tiny-invariant": "1.3.1",
        "tinykeys": "2.1.0",
        "ts-pattern": "5.0.1",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@babel/core": "7.22.6",
        "@tailwindcss/typography": "0.5.9",
        "@types/node": "20.3.3",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@types/throttle-debounce": "5.0.0",
        "@vanilla-extract/css": "1.12.0",
        "@vanilla-extract/vite-plugin": "3.8.2",
        "@vitejs/plugin-basic-ssl": "1.0.1",
        "@vitejs/plugin-react": "4.0.1",
        "autoprefixer": "10.4.14",
        "consola": "3.2.2",
        "core-dts": "0.0.3",
        "dpdm": "3.13.1",
        "esbuild": "0.18.11",
        "eslint": "8.44.0",
        "eslint-config-with-tsconfig": "1.6.10",
        "hkt-toolbelt": "0.22.0-beta.1",
        "husky": "8.0.3",
        "local-pkg": "0.4.3",
        "postcss": "8.4.24",
        "rollup": "3.26.1",
        "rollup-plugin-visualizer": "5.9.2",
        "rome": "12.1.3",
        "sass": "1.63.6",
        "serve": "14.2.0",
        "tailwindcss": "3.3.2",
        "tailwindcss-animate": "1.0.6",
        "ts-essentials": "9.3.2",
        "ts-unused-exports": "9.0.5",
        "tsx": "3.12.7",
        "typedoc": "0.24.8",
        "typesafe-i18n": "5.24.3",
        "typescript": "5.1.6",
        "vite": "4.3.9",
        "vite-plugin-checker": "0.6.1",
        "vite-plugin-preload": "0.3.1",
        "vite-plugin-top-level-await": "1.3.1",
        "vite-plugin-wasm": "3.2.2",
        "zx": "7.2.2"
    },
    "//": "dedupe packages for pnpm",
    "pnpm": {
        "overrides": {
            "@mantine/core": "6.0.14",
            "@mantine/hooks": "6.0.14",
            "highlight.js": "latest",
            "@codemirror/lang-markdown": "latest",
            "@codemirror/language-data": "latest",
            "@codemirror/state": "latest",
            "@codemirror/view": "latest",
            "react": "canary",
            "react-dom": "canary",
            "typescript": "latest"
        }
    }
}
