{
    "name": "botcraft",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "pnpm run i18n --no-watch && vite build",
        "check": "tsx ./scripts/check.ts",
        "clean": "rm -rf dist node_modules pnpm-lock.yaml",
        "dev": "vite",
        "format": "rome format . --write && pnpm check",
        "i18n": "typesafe-i18n",
        "preinstall": "npx only-allow pnpm",
        "lint": "rome format . --write && pnpm eslint --color src --fix",
        "list-unused": "ts-unused-exports ./tsconfig.json --excludePathsFromReport=src/i18n --exitWithCount",
        "prepare": "husky install",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit --extendedDiagnostics -p ."
    },
    "browserslist": [
        "defaults and supports es6-module"
    ],
    "dependencies": {
        "@ariakit/react": "0.2.10",
        "@codemirror/lang-markdown": "6.2.0",
        "@codemirror/language-data": "6.3.1",
        "@codemirror/state": "6.2.1",
        "@codemirror/view": "6.14.0",
        "@dqbd/tiktoken": "1.0.7",
        "@esfx/async-deferred": "1.0.0",
        "@floating-ui/react": "0.24.3",
        "@mantine/core": "6.0.14",
        "@mantine/hooks": "6.0.14",
        "@ngneat/falso": "6.4.0",
        "@phosphor-icons/react": "2.0.10",
        "@radix-ui/react-slot": "1.0.2",
        "@react-hookz/web": "23.0.1",
        "@rizzzse/bimap": "1.2.1",
        "@scure/bip39": "1.2.0",
        "@swan-io/boxed": "1.0.2",
        "@swan-io/chicane": "1.4.0",
        "@total-typescript/ts-reset": "0.4.2",
        "@uiw/react-codemirror": "4.21.5",
        "bind-event-listener": "3.0.0",
        "clsx": "1.2.1",
        "cm6-theme-basic-light": "0.2.0",
        "codemirror": "6.0.1",
        "date-fns": "2.30.0",
        "deep-weak-map": "1.1.0",
        "framer-motion": "10.12.17",
        "highlight.js": "11.8.0",
        "idb-keyval": "6.2.1",
        "immer": "10.0.2",
        "jotai": "2.2.1",
        "jotai-game": "0.1.0",
        "jotai-immer": "0.2.0",
        "jotai-minidb": "0.0.6",
        "katex": "0.16.8",
        "ky": "0.33.3",
        "nominal-types": "0.2.0",
        "rambda": "7.5.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-error-boundary": "4.0.10",
        "react-hot-toast": "2.4.1",
        "react-hotkeys-hook": "4.4.0",
        "react-markdown": "8.0.7",
        "rehype-highlight": "6.0.0",
        "rehype-katex": "6.0.3",
        "remark-breaks": "3.0.3",
        "remark-gfm": "3.0.1",
        "remark-math": "5.1.1",
        "rsup-progress": "3.1.1",
        "rxjs": "7.8.1",
        "short-unique-id": "4.4.4",
        "telejson": "7.1.0",
        "throttle-debounce": "5.0.0",
        "tiny-invariant": "1.3.1",
